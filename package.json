{
  "name": "esm-service",
  "version": "1.0.0",
  "main": "src",
  "type": "module",
  "scripts": {
    "build": "babel src -d dist -x .cjs,.js --keep-file-extension -D",
    "build:watch": "npm run build -- -w -s",
    "start": "pm2 start ecosystem.config.cjs --only service-dev --env development",
    "debug": "pm2 start ecosystem.config.cjs --only service-debug --env development",
    "server": "pm2 start ecosystem.config.cjs --only service --env production",
    "lint": "eslint . --ext .js,.cjs,.mjs",
    "lint:fix": "npm run eslint -- --fix",
    "test": "mocha"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "core-js": "^3.9.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.0",
    "koa-logger": "^3.2.1",
    "koa-session": "^6.1.0",
    "koa-static": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/plugin-syntax-top-level-await": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@koa/router": "^10.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
